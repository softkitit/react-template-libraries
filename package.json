		"@storybook/react": "^6.2.8",
		"@svgr/rollup": "^5.5.0",
		"@svgr/webpack": "^5.5.0",
		"@testing-library/react-hooks": "^6.0.0",
		"@types/chart.js": "^2.9.34",
		"@types/color": "^3.0.2",
