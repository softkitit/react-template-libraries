{
	"name": "react-template-libraries",
	"version": "0.1.0",
	"license": "MIT",
	"private": true,
	"workspaces": [
		"packages/*"
	],
	"scripts": {
		"link": "lerna exec -- npm i --package-lock-only && lerna link",
		"build": "lerna run build --stream",
		"build:components": "cd packages/react-common-components && yarn run build",
		"build:services": "cd packages/react-common-services && yarn run build",
		"pack:components": "cd packages/react-common-components && yarn pack",
		"pack:services": "cd packages/react-common-services && yarn pack",
		"lint": "lerna run lint --stream",
		"lint:fix": "lerna run lint:fix --stream",
		"copy-img-assets": "cp -r packages/react-common-components/src/assets/img/ storybook-static/",
		"storybook": "start-storybook -s packages/react-common-components/src/assets/ -p 6006",
		"storybook:build": "build-storybook && yarn run copy-img-assets",
		"storybook:serve": "yarn run storybook:build && npx http-server storybook-static",
		"postinstall": "yarn run link"
	},
	"husky": {
		"hooks": {
			"pre-commit": "tsdx lint"
		}
	},
	"devDependencies": {
		"@babel/core": "^7.13.15",
